{
  "expo": {
    "name": "fe",
    "slug": "fe",
    "scheme": "flowerapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/1.png",
    "userInterfaceStyle": "light",
   
    "splash": {
      "image": "./assets/1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/1.png",
      "color": "#9c84e8",
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from My App",
      "iosDisplayInForeground": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splashDaisy.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/splashDaisy.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/splashDaisy.png",
          "color": "#9c84e8",
          "defaultChannel": "default",
          "sounds": [
            "./assets/notification-sound.wav",
            "./assets/notification-sound-other.wav"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2e798f45-b0a9-4d88-9e78-123e3528439c"
      }
    }
  }
}
